				                            <img src={{ga.icon}} alt="" class="icon-info">
				                        {{/bp-layout}}
				                        <p class="text-header-large">{{ga.rate}}%</p>
				                    {{/bp-layout}}

				                    {{bp-circle
				                        circleData=ga.circleData
				                        circleColor=ga.circleColor
				                        classNames='circle-area'
				                        }}
				                {{/bp-layout}}
				            {{/each}}
							{{#bp-layout direction='row' main='between' cross='center' classNames='eff-list p-4'}}
								{{#bp-layout direction='col' classNames='circle-desc'}}
