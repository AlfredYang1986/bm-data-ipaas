										<p class="text-p-small mb-2">转介绍</p>
									{{/bp-layout}}
									<p class="text-header-large">50<span class="text-p-small">个</span></p>
								{{/bp-layout}}
							{{/bp-layout}}
				        {{/bp-layout}}
				    {{/bp-layout}}

				    {{#bp-layout direction='col'}}
				        {{#bp-layout direction='row' main='between'  cross='center' classNames='bg-white title-area mt-4 mb-2'}}
				            <p class="text-header-medium">行为指标</p>
							<p class="text-p-small">单位时间 <span> -</span></p>
				        {{/bp-layout}}
				        {{#bp-layout direction='row' classNames='mb-2'}}
				            {{#bp-layout direction='col' classNames='bg-white p-4 mr-2 admin'}}
				                <p class="text-header-small mb-4">教学</p>
								{{#bp-layout direction='row'}}
									{{#bp-layout direction='row' cross='center' classNames='mr-4'}}
										<p class="text-p-secondary mb-4 mr-1 text-width">课堂教学时长</p>
										<img src={{model.iconInfo}} alt="" class="icon-info mb-4">
									{{/bp-layout}}
									<p class="text-p-secondary mr-4"><span class="text-header-medium">6</span>分钟/次</p>
								{{/bp-layout}}
								{{#bp-layout direction='row'}}
									{{#bp-layout direction='row' cross='center' classNames='mr-4'}}
										<p class="text-p-secondary mb-4 mr-1 text-width">课后点评及时率</p>
										<img src={{model.iconInfo}} alt="" class="icon-info mb-4">
									{{/bp-layout}}
									<p class="text-p-secondary mr-4"><span class="text-header-medium">90%</span></p>
								{{/bp-layout}}
								{{#bp-layout direction='row'}}
									{{#bp-layout direction='row' cross='center' classNames='mr-4'}}
										<p class="text-p-secondary mb-4 mr-1 text-width">教具使用规范</p>
										<img src={{model.iconInfo}} alt="" class="icon-info mb-4">
									{{/bp-layout}}
									<p class="text-p-secondary mr-4"><span class="text-header-medium">4</span>次</p>
								{{/bp-layout}}
				            {{/bp-layout}}
							{{#bp-layout direction='col' classNames='bg-white p-4 admin'}}
							   <p class="text-header-small mb-4">贡献</p>
							   {{#bp-layout direction='row'}}
								   {{#bp-layout direction='row' cross='center' classNames='mr-4'}}
									   <p class="text-p-secondary mb-4 mr-1 text-width">课件使用与优化</p>
									   <img src={{model.iconInfo}} alt="" class="icon-info mb-4">
								   {{/bp-layout}}
								   <p class="text-p-secondary mr-4"><span class="text-header-medium">100%</span></p>
							   {{/bp-layout}}
							   {{#bp-layout direction='row'}}
								   {{#bp-layout direction='row' cross='center' classNames='mr-4'}}
									   <p class="text-p-secondary mb-4 mr-1 text-width">教研辅助支持</p>
								   {{/bp-layout}}
								   <p class="text-p-secondary mr-4 ml-3"><span class="text-header-medium">8</span>次</p>
							   {{/bp-layout}}
						   {{/bp-layout}}
				        {{/bp-layout}}

						{{#bp-layout direction='col' classNames='bg-white p-4 mb-2'}}
				            <p class="text-header-small mb-4">最近课件播放停顿点</p>

							{{table-stall-point option=(action 'option')}}
				        {{/bp-layout}}

						{{#bp-layout direction='col' classNames='bg-white p-4 mb-4'}}
				            <p class="text-header-small mb-4">报表提交</p>
				            {{#bp-layout direction='row'}}
				                {{#bp-layout direction='col' classNames='reports'}}
				                    {{#bp-layout direction='row'}}
				                        <p class="text-p-primary mb-3 mr-4">报表数量</p>
				                        <p class="text-p-secondary">40份</p>
				                    {{/bp-layout}}
									{{#bp-layout direction='row'}}
				                        <p class="text-p-primary mb-3 mr-4">提交次数</p>
				                        <p class="text-p-secondary">40次</p>
				                    {{/bp-layout}}
				                {{/bp-layout}}
				                {{#bp-layout direction='col' classNames='reports'}}
				                    {{#bp-layout direction='row'}}
				                        {{#bp-layout direction='row' cross='center' classNames='mr-4'}}
				                            <p class="text-p-primary mb-3 mr-1 pl-4">提交及时率</p>
				                            <img src={{model.iconInfo}} alt="" class="icon-info mb-3">
				                        {{/bp-layout}}
				                        <p class="text-p-secondary mr-4">80%</p>
				                    {{/bp-layout}}
				                    {{#bp-layout direction='row'}}
				                        {{#bp-layout direction='row' cross='center' classNames='mr-4'}}
				                            <p class="text-p-primary mb-3 mr-1 pl-4">提交准确率</p>
				                            <img src={{model.iconInfo}} alt="" class="icon-info mb-3">
				                        {{/bp-layout}}
				                        <p class="text-p-secondary mr-4">90%</p>
				                    {{/bp-layout}}
				                {{/bp-layout}}
				            {{/bp-layout}}
				        {{/bp-layout}}

					{{/bp-layout}}

				    {{#bp-layout direction='col' classNames='bg-white mb-4 pb-3'}}
				        {{#bp-layout direction='row' classNames='bg-white title-area mt-3 mb-2'}}
				            <p class="text-header-medium">满意度</p>
				        {{/bp-layout}}
				        {{#bp-layout direction='row'}}
				            {{#bp-layout direction='col' cross='center' classNames='satisfaction pl-4'}}
				                <p class="text-p-small mb-2">退费率</p>
				                <p class="text-header-small">20%</p>
				            {{/bp-layout}}
				            {{#bp-layout direction='col' cross='center' classNames='satisfaction'}}
				                <p class="text-p-small mb-2 pl-4">客诉率</p>
				                <p class="text-header-small pl-4">20%</p>
				            {{/bp-layout}}
				        {{/bp-layout}}
				    {{/bp-layout}}

				{{/bp-layout}}
			{{/bp-layout}}
		{{/bp-layout}}
	{{else if (eq switchValue 3)}}
