			<p class="mb-3">660/10,000 <span>人</span> </p>
			{{#bp-progress classNames='progress' as |p|}}
				{{p.bar value=660 minValue=0 maxValue=10000}}
			{{/bp-progress}}
		{{/bp-layout}}
		{{#bp-layout direction="col" main="between" classNames="team bg-white p-4"}}
			<p class="mb-3 team-left">团队人数</p>
			<p class="mb-3 team-left">66<span>人</span> </p>
			<p class="team-left"><span>同比</span><span>20%</span></p>
		{{/bp-layout}}
	{{/bp-layout}}
