			{{#bp-layout direction="col" main="center" classNames="pr-4 border-right"}}
				<p class="keys mb-2">签单课时</p>
				<p class="values text-red">53,708<span class="text-minimum">小时</span></p>
			{{/bp-layout}}
			{{#bp-layout direction="col" main="center" classNames="px-4 border-right"}}
				<p class="keys mb-2">总消课时</p>
				<p class="values text-green">53,708<span class="text-minimum">小时</span></p>
			{{/bp-layout}}
			{{#bp-layout direction="col" main="center" classNames="px-4"}}
				<p class="keys mb-2">其他收益</p>
				<p class="values section-number-small">¥1,000,000</p>
			{{/bp-layout}}
		{{/bp-layout}}
	{{/bp-layout}}
	{{#bp-layout main="between" classNames="teams mb-4"}}
